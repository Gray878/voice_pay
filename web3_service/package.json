{
  "name": "voice-to-pay-web3-service",
  "version": "1.0.0",
  "description": "Web3 Service for Voice-to-Pay with OrderBook",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "ts-node src/main.ts",
    "compile": "hardhat compile",
    "deploy:mumbai": "hardhat run scripts/deploy.js --network mumbai",
    "deploy:localhost": "hardhat run scripts/deploy.js --network localhost",
    "verify": "node scripts/verify-deployment.js",
    "example": "node examples/orderbook-usage.js",
    "test": "hardhat test",
    "test:rpc": "node scripts/test-rpc.js",
    "node": "hardhat node"
  },
  "keywords": ["web3", "orderbook", "polygon", "mumbai"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "ethers": "^6.9.0",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "winston": "^3.11.0",
    "pg": "^8.11.3",
    "redis": "^4.6.12",
    "xstate": "^4.38.2",
    "axios": "^1.6.5"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@openzeppelin/contracts": "^5.0.1",
    "hardhat": "^2.19.4",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "@types/node": "^20.10.6",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/pg": "^8.10.9"
  }
}
